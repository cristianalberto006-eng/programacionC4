Algoritmo MostrarDiezNaturales
		
	
		Definir contador Como Entero;
		

		contador <- 1;
		
		Escribir "Los primeros 10 números naturales son:";
		

		Mientras contador <= 10 Hacer
		
			Escribir contador;
		
			contador <- contador + 1;
			
		FinMientras
		
		Escribir "Fin de la secuencia.";
		
FinAlgoritmo

